						div(class="proyecto-imagen col-md-2")
							img(src="http://lorempixel.com/245/215/city/6",alt="")
			div(class="row row-slide row-slide-proyectos hidden-sm hidden-md hidden-lg")
				div(class="col-md-12")
					div(class="proyectos-titulo")
						h2 Proyectos
					div(class="carousel slide", id="carousel-2", data-ride="carousel")
						div(class="carousel-inner", role="listbox")
							div(class="item active")
								img(src="http://lorempixel.com/400/400/city/1",alt="")
							div(class="item")
								img(src="http://lorempixel.com/400/400/city/2",alt="")
							div(class="item")
								img(src="http://lorempixel.com/400/400/city/3",alt="")
							div(class="item")
